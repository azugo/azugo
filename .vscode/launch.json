{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch (API service example)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "buildFlags": "-tags 'netgo osusergo'",
      "program": "${workspaceRoot}/examples/api/",
      "args": ["web", "-p", "3000"],
      "cwd": "${workspaceFolder}",
      "env": {
        "SERVER_URL": "http://localhost:3000",
        "ENVIRONMENT": "Development",
      }
    }
  ]
}
